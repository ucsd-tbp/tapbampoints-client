{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "flowtype"
  ],
  "rules": {
    "react/require-extension": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "no-console": [
      "warn",
      { "allow": ["warn", "error"] }
    ]
  }
}
